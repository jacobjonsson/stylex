{
  "name": "stylex",
  "version": "0.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*",
    "fixtures/*"
  ],
  "scripts": {
    "format": "yarn run prettier \"{packages,fixtures}\" --write",
    "test": "yarn run jest",
    "build:core": "yarn workspace @jacobjonsson/stylex build",
    "watch:core": "yarn workspace @jacobjonsson/stylex watch",
    "cra:build": "yarn build:core && yarn workspace @fixtures/cra build",
    "gatsby:develop": "yarn build:core && yarn workspace @fixtures/gatsby develop",
    "gatsby:build": "yarn build:core && yarn workspace @fixtures/gatsby build && yarn workspace @fixtures/gatsby serve"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "babel-jest": "^25.3.0",
    "babel-plugin-tester": "^9.0.1",
    "jest": "^25.2.3",
    "lerna": "^3.20.2",
    "prettier": "^2.0.4",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.4.0"
  },
  "prettier": {
    "tabWidth": 4,
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
