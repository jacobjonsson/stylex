{
    "compilerOptions": {
        "target": "ES2016",
        "module": "commonjs",
        "lib": ["dom", "es2017"],
        "allowJs": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "noImplicitUseStrict": true
    }
}
